
[run]
source = pyfarm

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
omit =
    *pyfarm*__init__*
    *pyfarm/master/templates/*
    *pyfarm/master/static/*
    *pyfarm/master/entrypoints/*
    *pyfarm/master/admin/baseview.py*
    *pyfarm/master/initial.py*

