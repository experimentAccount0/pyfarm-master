server {
    listen 80;
    server_name _;

    location / {
        try_files $uri @application;
    }
    
    location /static/ {
        alias <absolute path to your_clone/pyfarm/master/static/;
    }

    location @application {
      include uwsgi_params;
      uwsgi_pass unix:/tmp/pyfarm-master.sock;
    }
}
